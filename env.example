# Notion Integration
# Get your API key from: https://www.notion.so/my-integrations
NOTION_API_KEY=your_notion_api_key_here

# Your Notion database ID
# You can find this in your database URL: https://www.notion.so/{workspace}/{database_id}?v={view_id}
NOTION_DATABASE_ID=your_notion_database_id_here

# Webhook Security
# Generate a secure random string for webhook authentication
NOTION_WEBHOOK_SECRET=your_webhook_secret_here

# Vercel Deploy Hook
# Get this from your Vercel project settings > Git > Deploy Hooks
# Create a deploy hook and paste the URL here
VERCEL_DEPLOY_HOOK_URL=https://api.vercel.com/v1/integrations/deploy/your_deploy_hook_here 